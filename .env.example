# Example .env file - copy to .env and customize for your environment

# Database Configuration
DATABASE_URL=sqlite+aiosqlite:///./agent_manager.db

# API Security Configuration  
SERVER_API_TOKEN=your_secure_server_token_here
SECRET_KEY=your_jwt_secret_key_here

# LLM Configuration
OPENAI_API_KEY=your_openai_api_key_here
LLM_MODEL=gpt-4
LLM_MAX_TOKENS=2000
LLM_TEMPERATURE=0.1

# Server Configuration
HOST=0.0.0.0
PORT=8000
DEBUG=true
LOG_LEVEL=info